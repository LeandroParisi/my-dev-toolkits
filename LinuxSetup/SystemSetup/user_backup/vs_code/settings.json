{
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.format.enable": true,
  "jira-plugin.workingProject": "",
  "workbench.settings.openDefaultSettings": true,
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "cSpell.userWords": [
    "enjusdt",
    "fontawesome",
    "fortawesome",
    "linebreak",
    "Middlewares",
    "nextjs",
    "trpc",
    "websockets"
  ],
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.formatOnSave": true,
  "editor.largeFileOptimizations": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "explorer.confirmDelete": false,
  "[python]": {
    "editor.formatOnType": true
  },
  "jest.autoRevealOutput": "off",
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[csharp]": {
    "editor.maxTokenizationLineLength": 2500
  },
  "editor.inlineSuggest.enabled": true,
  "githubPullRequests.fileListLayout": "tree",
  "dotnet.server.useOmnisharp": true,
  "githubPullRequests.pullBranch": "never",
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "Codegeex.Privacy": true,
  "[sql]": {
    "editor.defaultFormatter": "cymonk.sql-formatter"
  },
  "[xml]": {
    "editor.defaultFormatter": "fabianlauer.vs-code-xml-format"
  },
  "google-translate.firstLanguage": "de-en",
  "vscodeGoogleTranslate.preferredLanguage": "English",
  "playwright.reuseBrowser": true,
  "workbench.colorTheme": "Default Dark Modern",
  "prisma.showPrismaDataPlatformNotification": false,
  "github.copilot.editor.enableAutoCompletions": true
}
